<h1>Sign in</h1>

<p id="notice"><%= notice %></p>


<%= form_tag session_path do %>
  <strong>Username</strong> <%= text_field_tag :username, params[:username] %> <br>
  <strong>Password</strong> <%= password_field_tag :password, params[:password] %>

  <%= submit_tag "Log in", class:"btn btn-primary" %>
<% end %>

<h2><%= @place.name %></h2>

<em> <p><%= @place.street %></p>
<p><%= @place.zip %> <%= @place.city %></p> </em> 

<iframe
  width="400"
  height="300"
  frameborder="0" style="border:0"
  src="https://www.google.com/maps/embed/v1/place?key=<%= ENV['Google_API_KEY'] %>
    &q=<%= @place.street %>,+<%= @place.city %>,+<%= @place.country %>">
</iframe>
